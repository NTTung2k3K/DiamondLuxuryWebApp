    <script>
        // Function to add 'active' class to the current page link
        function setActiveMenuItem() {
            // Get the current URL path
            var currentPath = window.location.pathname;
            console.log('Current Path:', currentPath); // Logging current path for debugging

            // Get all main menu links
            var menuLinks = document.querySelectorAll('.mainmenu__link');
            var activeFound = false;

            // Loop through each main menu link
            menuLinks.forEach(function (link) {
                console.log('Checking link:', link.getAttribute('href')); // Log each link for debugging
                if (link.getAttribute('href') === currentPath) {
                    link.parentElement.classList.add('active');
                    activeFound = true;
                    console.log('Active link found:', link); // Log active link found
                } else {
                    link.parentElement.classList.remove('active');
                }
            });

            // If active menu item not found in main links, check submenu links
            if (!activeFound) {
                var submenuLinks = document.querySelectorAll('.sub-menu a, .megamenu a');
                submenuLinks.forEach(function (sublink) {
                    if (sublink.getAttribute('href') === currentPath) {
                        var parentMenuItem = sublink.closest('.menu-item-has-children');
                        if (parentMenuItem) {
                            parentMenuItem.classList.add('active');
                            console.log('Active submenu link found:', sublink); // Log active submenu link found
                        }
                    }
                });
            }
        }

        // Run the function when the page loads
        document.addEventListener('DOMContentLoaded', setActiveMenuItem);
    </script>
   


